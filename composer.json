{
  "name": "verystar/very",
  "description": "verystar very demo",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "fifsky",
      "email": "xudong.cai@verystar.cn"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "git@git.verystar.cn:very/framework.git"
    }
  ],
  "require": {
    "php": ">=7.0",
    "very/framework": "3.0.*",
    "sentry/sentry": "^1.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "files": [
      "helpers/util.php"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}